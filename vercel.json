{
  "rewrites": [
    {
      "source": "/:locale/:slug*",
      "destination": "/:locale/:slug*"
    }
  ],
  "headers": [
    {
      "source": "/:locale/:slug*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
